program SiDoar;

uses
  Vcl.Forms,
  UnitPrincipal in 'UnitPrincipal.pas' {FormTLPrincipal},
  UnitCadDoador in 'UnitCadDoador.pas' {Form1},
  UnitDoacao in 'UnitDoacao.pas' {Form2};

