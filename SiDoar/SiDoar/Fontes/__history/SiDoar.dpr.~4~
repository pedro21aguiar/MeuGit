program SiDoar;

uses
  Vcl.Forms,
  UnitPrincipal in 'UnitPrincipal.pas' {FormTLPrincipal},
  UnitCadDoador in 'UnitCadDoador.pas' {FormTelaCadDoador},
  UnitDoacao in 'UnitDoacao.pas' {FormTelaDoacao};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TFormTLPrincipal, FormTLPrincipal);
  Application.CreateForm(TFormTelaCadDoador, FormTelaCadDoador);
  Application.CreateForm(TFormTelaDoacao, FormTelaDoacao);
  Application.Run;
end.
